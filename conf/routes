# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
#GET    /images/*file							    controllers.Static.at(path="/public/images",file)
GET     /images/:id                                 controllers.ApplicationController.image(id: String)
GET     /images/:folder/:id                         controllers.ApplicationController.imageByFolder(folder:String, id: String)
GET     /ckeditorimages/:id                         controllers.ApplicationController.ckeditorImage(id: String)
GET     /ckeditorimages/:f1/:id                     controllers.ApplicationController.ckeditorImageByFolder(f1:String, id: String)
GET     /ckeditorimages/:f1/:f2/:id                 controllers.ApplicationController.ckeditorImageByFolder2(f1:String, f2:String, id: String)
GET     /ckeditorimages/:f1/:f2/:f3/:id             controllers.ApplicationController.ckeditorImageByFolder3(f1:String, f2:String, f3:String, id: String)
GET     /ckeditorimages/:f1/:f2/:f3/:f4/:id         controllers.ApplicationController.ckeditorImageByFolder4(f1:String, f2:String, f3:String, f4:String, id: String)
GET     /ckeditorimages/:f1/:f2/:f3/:f5/:id         controllers.ApplicationController.ckeditorImageByFolder5(f1:String, f2:String, f3:String, f4:String, f5:String, id: String)
OPTIONS /*all                           		    controllers.ApplicationController.preflight(all)

